{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "plugins": [
      {
        "name": "typescript-styled-plugin",
        "lint":{
          "validProperties":["margin-vertical", "padding-vertical", "padding-horizontal", "margin-horizontal", "resize-mode", "text-align-vertical"]
        }
      }
    ]
  }
}
